<div id="controls">
  <div id="startstop">
    <button id="timerControl" data-action="start">Start</button>
    <button data-action="reset">Reset</button>


  </div>
  <div id="presets">
    <h2>Create Timer</h2>
    
    <button data-duration="10">10s</button>
    <button data-duration="30">30s</button>
    <button data-duration="45">45s</button>
    <button data-duration="60">60s</button>
  <div>

  
  <fieldset class="range__field">
    <label>Intervals</label>
    <input class="range" type="range" min="1" max="11" name="rounds" value="3">
    <svg role="presentation" width="100%" height="10" xmlns="http://www.w3.org/2000/svg">
        <rect class="range__tick" x="0%" y="3" width="1" height="10"></rect>
        <rect class="range__tick" x="10%" y="3" width="1" height="10"></rect>
        <rect class="range__tick" x="20%" y="3" width="1" height="10"></rect>
